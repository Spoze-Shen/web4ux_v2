					$("#loader").addClass(".fadeOut");
					$('#create-project-modal').modal('toggle');
				});
			}
		});
		$("#select-all-manager-btn").click(function() {
			if ($(this).hasClass("selected")) {
				$("#manager-select .radio-group").removeClass("active");
				$(this).removeClass("selected");
				$(this).html("Select");
			} else {
				$("#manager-select .radio-group").addClass("active");
				$(this).addClass("selected");
				$(this).html("Unselect");
			}
		});
		$(".permission-radio").click(function() {
			if ($("#everyone-permission").hasClass("active")) {
				$("#everyone-permission").removeClass("active");
				$("#onlyme-permission").addClass("active");
				$(".manager-select-group .cover").hide();
				newProject.CreaterEditorOnly = true;
			} else {
				$("#onlyme-permission").removeClass("active");
				$("#everyone-permission").addClass("active");
				$("#manager-select .radio-group").removeClass("active");
				$(this).removeClass("selected");
				$(".manager-select-group .cover").show();
				newProject.CreaterEditorOnly = false;
			}
		});
		$("#caculate-id-btn").click(function() {
			$(".fitts-d").each(function(index) {
				calculateId(index);
				needCalculate = false;
				$("#create-project-btn").prop( "disabled", false );
				$("#need-calculate-alert").hide();
			});
		});
		$(".test-order-card .remove-test-btn").click(function() {
			$(this).parent().removeClass("active");
			$(this).parent().find(".check-test-btn").show();
			$(this).parent().find(".sort-test-btn-group").hide();
			$(this).hide();
			resetTestCard();
		});
		$(".test-order-card .check-test-btn").click(function() {
			$(this).parent().addClass("active");
			$(this).parent().find(".remove-test-btn").show();
			$(this).parent().find(".sort-test-btn-group").show();
			$(this).hide();
			resetTestCard();
		});
		$(".sort-test-btn-group button").click(function() {
			var test = $(this).parent().attr("data-test"),
				value = $(this).attr("data-value");
			$(".card-footer[data-value=" + value + "]").html("#");
			$(".card-footer[data-value=" + value + "]").attr("data-value", "#");
			$(this).parent().find("button").removeClass("active");
			$(".sort-test-btn-group button[data-value=" + value + "]").removeClass("active");
			$(".card-footer[data-name='" + test + "']").html(indexText[parseInt(value) - 1]);
			$(".card-footer[data-name='" + test + "']").attr("data-value", value);
			$(this).addClass("active");
		});
	}

	function calculateId(iIndex) {
		var W = parseFloat($(".fitts-w").eq(iIndex).val()),
			D = parseFloat($(".fitts-d").eq(iIndex).val());
		var newID = (D / W) + 1;
		$(".fitts-id").eq(iIndex).val(parseFloat((Math.log2(newID)).toFixed(1)));
	}
</script>
