	function resetTestCard() {
		$(".test-order-card.active .sort-test-btn-group").html("");
		$(".test-order-card.active").each(function(index) {
			$("<button />", {
				type: "button",
				class: "btn btn-outline-success",
				"data-value": "" + (index + 1),
				text: indexText[index]
			}).appendTo(".test-order-card.active .sort-test-btn-group").click(function() {
				var test = $(this).parent().attr("data-test"),
					value = $(this).attr("data-value");
				$(".card-footer[data-value=" + value + "]").html("#");
				$(".card-footer[data-value=" + value + "]").attr("data-value", "#");
				$(this).parent().find("button").removeClass("active");
				$(".sort-test-btn-group button[data-value=" + value + "]").removeClass("active");
				$(".card-footer[data-name='" + test + "']").html(indexText[parseInt(value) - 1]);
				$(".card-footer[data-name='" + test + "']").attr("data-value", value);
				$(this).addClass("active");
			});
		});
		$(".sort-test-btn-group button").removeClass("active");
		$(".card-footer").html("#");
		$(".card-footer").attr("data-value", "#");
	}

